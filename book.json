{
  "gitbook": ">3.0.0",
  "plugins": [
    "theme-vuejs@git+https://github.com/pearofducks/gitbook-plugin-theme-vuejs.git",
    "github",
    "-fontsettings"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/gdutwyg"
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  },
  "pdf": {
    "pageNumbers": false,
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
        "right": 62,
        "left": 62,
        "top": 36,
        "bottom": 36
    },
    "headerTemplate": null,
    "footerTemplate": null
  }
}